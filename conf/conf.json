{
  "ip": "127.0.0.1",
  "port": "8080",

  "my_sql_db_name": "goteamroom",

  "MAX_MESSAGES_AT_ONCE": 1000,
  "MAX_CHATROOMS": 1000

  "out": {
    "name": "stdout"
  },
